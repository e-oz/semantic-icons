{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@semantic-icons/flag-icons/rectangle": [
        "libs/flag-icons/rectangle/src/index.ts"
      ],
      "@semantic-icons/flag-icons/square": [
        "libs/flag-icons/square/src/index.ts"
      ],
      "@semantic-icons/flowbite-icons/outline": [
        "libs/flowbite-icons/outline/src/index.ts"
      ],
      "@semantic-icons/flowbite-icons/solid": [
        "libs/flowbite-icons/solid/src/index.ts"
      ],
      "@semantic-icons/heroicons/16/solid": [
        "libs/heroicons/16/solid/src/index.ts"
      ],
      "@semantic-icons/heroicons/20/solid": [
        "libs/heroicons/20/solid/src/index.ts"
      ],
      "@semantic-icons/heroicons/24/outline": [
        "libs/heroicons/24/outline/src/index.ts"
      ],
      "@semantic-icons/heroicons/24/solid": [
        "libs/heroicons/24/solid/src/index.ts"
      ],
      "@semantic-icons/lucide-icons": ["libs/lucide-icons/src/index.ts"],
      "@semantic-icons/nx-generators": ["libs/nx-generators/src/index.ts"],
      "@semantic-icons/tabler-icons/filled": [
        "libs/tabler-icons/filled/src/index.ts"
      ],
      "@semantic-icons/tabler-icons/outline": [
        "libs/tabler-icons/outline/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
